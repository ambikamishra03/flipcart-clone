{
  "name": "server",
  "version": "1.0.0",
  "description": "flipcart clone",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "frontend": "npm start --prefix frontend",
    "frontendinstall": "npm install --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",

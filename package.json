  "dependencies": {
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "formidable": "^3.5.1",
    "https": "^1.0.0",
    "mongodb": "^5.7.0",
    "mongoose": "^7.4.1",
    "node": "^20.5.1",
    "nodemon": "^3.0.1",
    "path": "^0.12.7",
    "uuid": "^9.0.0"
  }
}
